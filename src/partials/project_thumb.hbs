<div class="thumbnail">
    <div class="img_container">
        <img src="./img/{{project.img}}" alt="project thumbnail">
        <div class="overlay_container">
            <div>
                <a href="{{project.visit}}" target="blank" {{#if (isNull project.visit)}}class="unavailable"{{/if}}>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 61.02 61.02"><path d="M30.51,20.29a10.22,10.22,0,0,0,0,20.44h0a10.22,10.22,0,0,0,0-20.44Zm6.48,11a6.53,6.53,0,1,1-13-1.54,5,5,0,0,0,7.38-5.69A6.52,6.52,0,0,1,37,31.28Z"/><path d="M30.51,0A30.51,30.51,0,1,0,61,30.51,30.51,30.51,0,0,0,30.51,0ZM51,31.56c-2.27,2.93-10.45,12.51-20.53,12.51S12.25,34.49,10,31.56a1.71,1.71,0,0,1,0-2.1C12.25,26.53,20.43,17,30.51,17S48.77,26.53,51,29.46A1.71,1.71,0,0,1,51,31.56Z"/></svg>
                    {{#if (isNull project.visit)}}<p>unavailable</p>{{else}}<p>visit</p>{{/if}}
                </a>
            </div>
            <div>
                <a href="{{project.source}}" target="blank" {{#if (isNull project.source)}}class="unavailable"{{/if}}>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32.58 31.77"><path class="cls-1" d="M16.29,0a16.29,16.29,0,0,0-5.15,31.75c.82.15,1.11-.36,1.11-.79s0-1.41,0-2.77C7.7,29.18,6.74,26,6.74,26a4.36,4.36,0,0,0-1.81-2.39c-1.47-1,.12-1,.12-1a3.43,3.43,0,0,1,2.49,1.68,3.48,3.48,0,0,0,4.74,1.36,3.46,3.46,0,0,1,1-2.18c-3.62-.41-7.42-1.81-7.42-8a6.3,6.3,0,0,1,1.67-4.37,5.94,5.94,0,0,1,.16-4.31s1.37-.44,4.48,1.67a15.41,15.41,0,0,1,8.16,0c3.11-2.11,4.47-1.67,4.47-1.67A5.91,5.91,0,0,1,25,11.07a6.3,6.3,0,0,1,1.67,4.37c0,6.26-3.81,7.63-7.44,8a3.85,3.85,0,0,1,1.11,3c0,2.18,0,3.94,0,4.47s.29.94,1.12.78A16.29,16.29,0,0,0,16.29,0Z"/></svg>
                    {{#if (isNull project.source)}}<p>unavailable</p>{{else}}<p>source</p>{{/if}}
                </a>
            </div>
            <div>
                <a href="./projects/{{flattenString project.title}}">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 61.02 61.02"><path d="M30.51,0A30.51,30.51,0,1,0,61,30.51,30.51,30.51,0,0,0,30.51,0Zm4.93,44.71a2,2,0,0,1-.44.42,13.82,13.82,0,0,1-7.87,2.57c-2.62,0-4-1.81-3.44-4.33.72-3,1.47-6,2.21-9a2.25,2.25,0,0,0,0-1.67,1.86,1.86,0,0,0-1-.76,2.62,2.62,0,0,0-.68-.12c-.28-.07-.69-.11-.89-.42l-.06-.09h0a1,1,0,0,1,.26-1.3l.52-.34.78-.44c.55-.27,1-.48,1.42-.63a17.2,17.2,0,0,1,1.95-.66c.34-.09.71-.18,1.12-.25a15.65,15.65,0,0,1,1.92-.21,6.76,6.76,0,0,1,1.55.17,3.16,3.16,0,0,1,2.3,4.12c-.72,3.05-1.49,6.09-2.23,9.13-.35,1.43,0,2.07,1.45,2.46.22.06.44.09.66.15C35.73,43.63,35.91,44.1,35.44,44.71ZM33,23.12a4.89,4.89,0,0,1-4.85-4.94V18A4.8,4.8,0,0,1,33,13.33h.08A4.9,4.9,0,0,1,33,23.12Z"/></svg>
                    <p>info</p>
                </a>
            </div>
        </div>
    </div>
    <div class="desc_container">
        <span class="title">{{project.title}}</span>
        <span class="desc">{{project.desc}}</span>
        <span class="year">{{project.year}}</span>
        <div class="tags">
            {{#each (commaSplit project.short-tags)}}
            <span class="tag {{flattenString this}}">{{this}}</span>
            {{/each}}
        </div>
    </div>
</div>